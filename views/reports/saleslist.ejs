<% if (allrecs.length > 0) {%>
    <table>
        <tr>
            <th>Order ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Product Name</th>
            <th>Quantity</th>
            <th>Subtotal</th>
            <th>Order Date</th>
        </tr>
      <% allrecs.forEach((recref, index) => { %>
        <tr> 
            <td><%= recref.orderid %></td>
            <td><%= recref.firstname %></td>
            <td><%= recref.lastname %></td>
            <td><%= recref.productname %></td>
            <td><%= recref.qty %></td>
            <td><%= Intl.NumberFormat('en-US', { style: 'currency', currency: 'USD' }).format(recref.subtotal) %> </b> <br/>%></td>
            <td><%= recref.orderdate.toISOString().split('T')[0] %></td>
            </tr>
    <% }); %>
    </table>
    <% } else { %>
        <p>Not Records Available</p>
    <% } %>



